.topBars{
  height: 100rpx;
  width: 100%;
  z-index: 999;
  position: fixed;
  top:0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background-color: #ccfbff;
}
.topBar-item0{
  height:100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.topBar-item0{
  height:100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgb(9, 247, 29);
}
.content{
  position: fixed;
  top: 110rpx;
  left: 0;
  height: 1500rpx;
  width: 100%;
  /* background-color: #d13abd; */
}
.title{
  height: 100rpx;
  width: 100%;
  background-color: rgb(241, 239, 239);
  position: fixed;
  top: 128rpx;
  left: 0;
  z-index: 999;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0 20rpx;
}

.title text{
  line-height: 70rpx;
  width: 70%;
  display:-webkit-box; 
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient:vertical; 
  -webkit-line-clamp: 1;
}

.btn{
  height: 76rpx;
  width: 20%;
  text-align: center;
  line-height: 70rpx;
  color: rgb(131, 131, 131);
  font-size: 35rpx;
}

.item{
  margin: 8rpx 0;
  height: 150rpx;
  background-color: rgb(231, 235, 253);
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}
.root0{
  background-color: gainsboro;
  position: relative;
  top: 130rpx;
  left: 0;
}
.root1{
  background-color: gainsboro;
  position: relative;
  top: 100rpx;
  left: 0;
}
.tip{
  /* height: 36rpx; */
  font-size: 28rpx;
  background-color: rgb(248, 246, 98);
  position: fixed;
  z-index: 999;
  display: flex;
  width: 100%;
  align-items: center;
  justify-content: center;
  top: 100rpx;
  left: 0;
}